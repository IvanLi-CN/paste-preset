{
  "name": "paste-preset",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/write-version.mjs && vite",
    "build": "tsc -b && vite build && node scripts/write-version.mjs",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev -p 6006 --ci --disable-telemetry --no-open",
    "build-storybook": "storybook build",
    "test-storybook:runner": "test-storybook --url http://127.0.0.1:6006",
    "test-storybook": "start-server-and-test storybook:ci http://127.0.0.1:6006 test-storybook:runner"
  },
  "dependencies": {
    "@iconify/react": "^6.0.2",
    "exifr": "^7.1.3",
    "heic2any": "^0.0.4",
    "jszip": "^3.10.1",
    "piexifjs": "1.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "10.1.6",
    "@storybook/addon-docs": "10.1.6",
    "@storybook/addon-links": "10.1.6",
    "@storybook/jest": "0.2.3",
    "@storybook/react-vite": "10.1.6",
    "@storybook/test-runner": "^0.24.0",
    "@storybook/testing-library": "0.2.2",
    "@tailwindcss/vite": "^4.1.17",
    "@types/bun": "^1.3.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.5.5",
    "jsdom": "^27.0.0",
    "lefthook": "^2.0.4",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "start-server-and-test": "^2.1.3",
    "storybook": "10.1.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^2.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
